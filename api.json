                        async def parse_emote_command(command_text):
                            """
                            1. /e emote_id ‚Üí sends to sender
                            2. /e emote_name ‚Üí sends to sender
                            3. /e uid emote_id ‚Üí sends to specific uid
                            4. /e uid emote_name ‚Üí sends to specific uid
                            5. /e uid1 uid2 uid3 emote_id ‚Üí sends to multiple uids
                            6. /e uid1 uid2 uid3 emote_name ‚Üí sends to multiple uids
                            
                            Returns: (target_uids, emote_id, error_message)
                            """
                            parts = command_text.strip().split()
                            
                            if len(parts) < 2:
                                return None, None, "[C][B][FF0000]Invalid format!\nUsage: /e [uid(s)] [emote_id/name]"
                            
                            args = parts[1:]
                            
                            last_arg = args[-1]
                            
                            emote_id = None
                            if last_arg in emote_map:
                                emote_id = emote_map[last_arg]
                            elif last_arg.isdigit() and len(last_arg) == 9:
                                emote_id = last_arg
                            else:
                                return None, None, f"[C][B][FF0000]Invalid emote: {last_arg}\nUse emote ID or name"
                            
                            target_uids = []
                            uid_args = args[:-1]
                            
                            if len(uid_args) == 0:
                                return None, emote_id, None
                            
                            for uid_str in uid_args:
                                if uid_str.isdigit():
                                    target_uids.append(int(uid_str))
                                else:
                                    return None, None, f"[C][B][FF0000]Invalid UID: {uid_str}"
                            
                            return target_uids, emote_id, None
                        
                        
